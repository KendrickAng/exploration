{
  "baseUrl": "/exploration",
  "faviconPath": "favicon.png",
  "titlePrefix": "CP3106",
  "ignore": [
    "_markbind/layouts/*",
    "_markbind/logs/*",
    "_site/*",
    "site.json",
    "*.md",
    "*.mbd",
    "*.mbdf",
    "*.njk",
    ".git/*"
  ],
  "pages": [
    {
      "src": "index.md",
      "title": "Kendrick's CP3106 Blog"
    },
    {
      "glob": ["**/index.md", "**/*.+(md|mbd)"]
    }
  ],
  "deploy": {
    "message": "Site Update."
  },
  "plugins": ["googleAnalytics"],
  "pluginsContext": {
    "googleAnalytics": {
      "trackingID": "UA-175048156-1"
    }
  }
}
